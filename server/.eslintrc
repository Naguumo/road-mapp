{
  "env": {
    "node": true,
    "es6": true
  },
  "extends": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["import", "prettier"],
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "no-unused-vars": 1,
    "import/no-unresolved": 0,
    "import/extensions": 0
  }
}
