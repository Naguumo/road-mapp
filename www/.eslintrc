{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier", "react-app"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["jsx-a11y", "prettier", "react", "react-hooks"],
  "rules": {
    "no-console": 0,
    "no-param-reassign": 0,
    "no-unused-vars": [
      1,
      {
        "varsIgnorePattern": "[rR]eact"
      }
    ],
    "import/no-unresolved": 0,
    "import/extensions": 0,
    "react/jsx-curly-newline": 0,
    "react/prop-types": [1, {}]
  }
}
